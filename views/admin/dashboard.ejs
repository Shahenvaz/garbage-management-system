<%- include('../header'); -%>



    <div class="limiter" style="margin-top:150px">
        <div class="container-login100">
            <div class="wrap-login100">
                <div style="position: fixed; top: 250px; color: red; font-size: xx-large;">
                    All admin details
                </div>
            <% for(var i=0; i<data.length; i++) {%>
                 
                <div>
                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" src="https://www.pngmart.com/files/21/Admin-Profile-PNG-Clipart.png" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title"> Name : <%= data[i]['admin-name'] %> </h5>
                            <a href="#" class="btn btn-primary"> EMAIL : <%= data[i]['admin-email'] %></a>
                        </div>
                    </div>
                </div>
            <% } %>
            </div>
        </div>
    </div>










